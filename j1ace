adb shell input keyevent 3
adb shell input keyevent 187
sleep 2
adb shell input tap 234 764
sleep 2
adb shell input keyevent 3
sleep 2
adb shell am start -n com.android.settings/com.android.settings.deviceinfo.SimStatus

sleep 2
adb shell screencap /sdcard/cek_sinyal_disini.png
sleep 1
rm -r /root/.000j1ace
sleep 2
mkdir /root/.000j1ace
adb pull /sdcard/cek_sinyal_disini.png /root/.000j1ace
sleep 1
