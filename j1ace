1) clear ; 
echo -e mohon ditunggu proses lock auto freq 4g....
sleep 5
adb shell input keyevent 3
adb shell input keyevent 187
sleep 2
adb shell input tap 234 764
sleep 2
adb shell input keyevent 3
sleep 2

adb shell su -c am broadcast -a android.provider.Telephony.SECRET_CODE -d android_secret_code://2263
sleep 4

adb shell input tap 121 188
sleep 1
adb shell input tap 102 354
sleep 1
adb shell input tap 100 186

sleep 3
adb shell screencap /sdcard/cek_frekuensi.png
rm -r /root/.000j1ace
sleep 2
mkdir /root/.000j1ace
adb shell input keyevent 26
adb pull /sdcard/cek_frekuensi.png /root/.000j1ace
sleep 2
clear
echo -e 
echo -e Done................................
echo -e
echo -e Silahkan cek screenshot berhasil tidaknya lockband di folder /root/.000j1ace
 ;;

