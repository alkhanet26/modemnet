echo -e
echo -e "\e[33mSILAHKAN CEK SINYAL DI FOLDER PALING LUAR FILE MANAGER,\033[0m" nama foldernya /root/.000j1ace
 ;;

9) clear ; 
echo -e Proses ubah ip, mohon ditunggu
echo -e
adb shell settings put global airplane_mode_on 1
adb shell am broadcast -a android.intent.action.AIRPLANE_MODE
sleep 4
adb shell settings put global airplane_mode_on 0
adb shell am broadcast -a android.intent.action.AIRPLANE_MODE
clear
sleep 1
echo -e 
echo -e "\e[33mIP SUDAH BERUBAH BOSKUH\033[0m", Silahkan Cek IP Barunya
