10) clear ; 
echo -e
adb shell netcfg
echo -e
echo -e "\e[33mCEK IP DIBARISAN rmnet0\033[0m"
 ;;

x) exit ;;
*) echo "Anda salah tekan " ; sleep 1 ; sh /sbin/j1ace ;;
esac