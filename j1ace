adb shell screencap /sdcard/cek_frekuensi.png
rm -r /root/.000j1ace
sleep 2
mkdir /root/.000j1ace
adb shell input keyevent 26
adb pull /sdcard/cek_frekuensi.png /root/.000j1ace
sleep 2
